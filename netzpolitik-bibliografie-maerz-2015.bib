@video{alex_berlin_ist_2016,
  location =	 {Berlin},
  title =	 {Ist die Netzneutralität in Europa noch zu retten?},
  url =		 {https://www.youtube.com/watch?v=08HMUIf2pw0},
  abstract =	 {Im Oktober 2015 hat das Europa-Parlament
                  Netzneutralitätsregeln beschlossen, die am 30. April
                  diesen Jahres in allen Mitgliedsstaten der {EU} in
                  Kraft treten. Die Regeln sind in entscheidenden
                  Fragen so vage, dass unklar ist, ob sie die
                  Netzneutralität gewährleisten oder abschaffen. Die
                  Aufgabe, diese Fragen zu klären, kommt jetzt den
                  Europäischen Regulierungsbehörden für Elektronische
                  Kommunikation zu: Das Netzneutralitätsgesetz gibt
                  ihnen den Auftrag, bis Ende August diesen Jahres
                  Richtlinien zu erlassen, die eine einheitliche
                  Anwendung der Netzneutralitätsregeln in Europa
                  gewährleisten sollen. Der Vortrag erklärt, welche
                  Fragen noch offen sind und was für Deutschland und
                  Europa auf dem Spiel steht.},
  publisher =	 {{ALEX} Berlin},
  author =	 {{ALEX Berlin} and Schewick, Barbara van},
  urldate =	 {2018-04-21},
  date =	 {2016-05-02}
}

